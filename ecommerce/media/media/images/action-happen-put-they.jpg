function createColorModificationFunction(targetColor, excludeColor) {
  const isColorEqual = (colorA, colorB) => {
    return colorA.every((value, index) => Math.abs(value - colorB[index]) < 0.01);
  };

  return function modifyColors(animationData) {
    animationData.layers.forEach(layer => {
      if (layer.shapes && Array.isArray(layer.shapes)) {
        layer.shapes.forEach(shape => {
          if (shape.it && Array.isArray(shape.it)) {
            shape.it.forEach(item => {
              if (item.ty === "st" && item.c && item.c.k) {
                if (!isColorEqual(item.c.k, excludeColor)) {
                  item.c.k = targetColor;
                }
              }
            });
          }
        });
      }
    });
    return animationData;
  };
}

