  $('.card-slider').slick('slickNext');
})

cardSlider.on('afterChange', function(event, slick, currentSlide) {
  // Disable the previous button on the first slide
  if (currentSlide === 0) {
   $('.card-prev').addClass('disabled'); 
  }
  // Enable the previous button on other slides
  if (currentSlide !== 0) {
   $('.card-prev').removeClass('disabled'); 
  }
  // Disable the next button on the last slide
  if (slick.slideCount === currentSlide + 1) {
    $('.card-next').addClass('disabled'); 
  }
});

